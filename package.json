{
  "name": "sint",
  "version": "0.0.3",
  "description": "A Spreadsheet language interpreter",
  "main": "./lib/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
    "docs": "jsdoc src -r -c scripts/jsdoc.conf.json -d docs -R README.md",
    "dist": "mkdir -p ./dist && npm run babel && npm run browserify",
    "babel": "babel src -d lib",
    "browserify": "browserify lib/index.js -t [ babelify --comments false ] -t uglifyify --outfile dist/sint.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LucasNevesAraujo/Sint.git"
  },
  "keywords": [
    "javascript",
    "nodejs",
    "interpreter",
    "spreadsheet",
    "es6"
  ],
  "readme": "README.md",
  "files": [
    "dist/",
    "lib/",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "author": "Lucas Araujo <araujo.lucas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LucasNevesAraujo/Sint/issues"
  },
  "homepage": "https://github.com/LucasNevesAraujo/Sint#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-class-display-name": "^2.1.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "eslint": "^4.1.1",
    "expect.js": "^0.3.1",
    "jsdoc": "^3.4.3",
    "mocha": "^3.4.2",
    "uglifyify": "^4.0.2"
  },
  "dependencies": {
    "object.values": "^1.0.4",
    "formulajs": "^1.0.8"
  }
}
